<div class="row">
  <div class="col-xs-12">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">List of All Ticket Debits</h3> 
        </div>
        <div class="box-body">
          <div id="user_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">

            <div class="row">
              <div class="col-sm-12">
               <!--
                <table id="user_index " class="table table-bordered table-striped dataTable" role="grid"> 
                -->
              <table id="user_index" class="table table-bordered table-striped dataTables-example table-responsive" role="grid">                  <thead>
                    <tr>
                      <th> Ref#</th>
                      <th> Name </th>
                      <th> Phone</th>
                      <th> Paid Amount</th>
                      <th> Debit</th>
                      <th> Status </th>
                      <th> Date </th>
                      <th class="col-xs-1"> 
                      Action</th>
                    </tr>
                  </thead>

                  <tbody>
                    <% @tickets.each do |ticket| %>
                      <tr>
                        <td> <%= ticket.id %> </td>
                        <td> <%= ticket.name %> </td>
                       
                       <td> <%= ticket.phone %> </td>
                        <td> <%= ticket.paidAmount %> </td>                     
                        <td> <%= ticket.debit %> </td>
                        <td> <%= ticket.status %> </td>
                        <td> <%= ticket.updated_at.strftime("%d-%m-%Y") %> </td>

                        <td>
                          <div class="btn-row">
                            <%= link_to raw('<i class="fa fa-eye"></i>'), payment_edit_admin_ticket_url(ticket), class: 'btn btn-sm btn-primary' %>
                          </div>
                        </td>
                      </tr>
                    <% end %>
                  </tbody>

                 
                </table>
              </div>
            </div>

          </div>
        </div>

    </div>
  </div>
</div>
