<div class="row">
  <div class="col-xs-12">
    <div class="box">
              <div class="box-header">
                <h3 class="box-title"><%=@company.name%></h3>
                <%= link_to "view report", preview_admin_company_path(@company),class: "btn btn-large btn-info fa fa-report pull-right", style:"margin-left: 22px;" %>
              </div>
              <!-- /.box-header -->
              <div class="box-body no-padding">
                <table class="table table-striped">
                  <% for attribute in @company.attributes.keys do  %>
                    <tr>
                      <th style="width:200px;"> <%= attribute.humanize%></th>
                      <td> <%= @company.attributes[attribute].to_s %> </td>
                    </tr>
                  <% end %>
                </table>
              </div>
              <!-- /.box-body -->
            </div>
          </div>
        </div>